{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "typeRoots": [
      "node_modules/@types",
      "types"
    ]
  }
}